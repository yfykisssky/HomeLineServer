<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>HomeLine</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<listener>
		<listener-class>com.homeline.StartListener</listener-class>
	</listener>
	<servlet>
		<servlet-name>SendCodeServlet</servlet-name>
		<servlet-class>com.homeline.servlet.SendCodeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SendCodeServlet</servlet-name>
		<url-pattern>/sendcode</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>GetPubKeyServlet</servlet-name>
		<servlet-class>com.homeline.servlet.GetPubKeyServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetPubKeyServlet</servlet-name>
		<url-pattern>/getpubkey</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.homeline.servlet.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>LoginOutServlet</servlet-name>
		<servlet-class>com.homeline.servlet.LoginOutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginOutServlet</servlet-name>
		<url-pattern>/loginout</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>GetDataServlet</servlet-name>
		<servlet-class>com.homeline.servlet.GetDataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetDataServlet</servlet-name>
		<url-pattern>/getdata</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>GetEnvironmentDataServlet</servlet-name>
		<url-pattern>/getenvironmentdata</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>GetEnvironmentDataServlet</servlet-name>
		<servlet-class>com.homeline.servlet.GetEnvironmentDataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TestServlet</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>TestServlet</servlet-name>
		<servlet-class>com.homeline.servlet.TestServlet</servlet-class>
	</servlet>
</web-app>